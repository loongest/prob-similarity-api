{
    "version": 2,
    "builds": [
      {
        "src": "similarity.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": ["**/*.py", "requirements.txt"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "similarity.py"
      }
    ]
  }